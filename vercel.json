{
    "version": 2,
    "functions": {
        "src/server.js": {
            "runtime": "vercel-node@latest"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/src/server.js"
        },
        {
            "source": "/app",
            "destination": "/index.html"
        },
        {
            "source": "/",
            "destination": "/landing.html"
        }
    ]
}